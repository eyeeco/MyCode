class Solution {
public:
    int findContentChildren(vector<int>& g, vector<int>& s) {
        sort(g.begin(),g.end(), [](int a,int b){return a>b;});
        sort(s.begin(),s.end(), [](int a,int b){return a>b;});
        int num = 0,i=0,j=0;
        while(i<g.size()&&j<s.size()){
            if(g[i]<=s[j]){
                j++;
                num++;
            }
            i++;
        }
        return num;
    }
};